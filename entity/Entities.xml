<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    <entity entity-name="netdisk.Bucket" package="netdisk">
        <field name="bucketId" type="id" is-pk="true"/>
        <field name="userId" type="id"/>
        <field name="quotaLimit" type="number"/>
        <field name="createdDate" type="date-time"/>
        <relationship type="one" title="User" related="moqui.security.UserAccount" key-map="userId=userId"/>
    </entity>
</entities>